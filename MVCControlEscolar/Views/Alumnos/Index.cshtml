@model IEnumerable<MVCControlEscolar.Models.Alumno>

@{
    ViewData["Title"] = "Index";
}

<h1>Alumnos</h1>

<p>
    <a asp-action="Create" class="btn btn-success">Nuevo Alumno</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @* @Html.DisplayNameFor(model => model.Matricula) *@
                Matr&iacute;cula
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.Nombre) *@
                Nombre
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.PrimerApellido) *@
                Primer Apellido
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.SegundoApellido) *@
                Segundo Apellido
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.CorreoElectronico) *@
                Correo Electr&oacute;nico
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.Telefono) *@
                Tel&eacute;fono
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.IdEstatusRegistroNavigation) *@
                Estatus de Registro
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimerApellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SegundoApellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoElectronico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstatusRegistroNavigation.Nombre)
            </td>
            <td>
                    <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-info">Consultar</a>&nbsp;
                    <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary">Editar</a> &nbsp;
                    <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
