@model MVCControlEscolar.Models.Grupo

@{
    ViewData["Title"] = "Details";
}

<h1>Consultar</h1>

<div>
    <h4>Grupo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @* @Html.DisplayNameFor(model => model.Nombre) *@
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @* @Html.DisplayNameFor(model => model.IdMateriaNavigation) *@
            Materia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMateriaNavigation.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @* @Html.DisplayNameFor(model => model.IdProfesorNavigation) *@
            Profesor
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdProfesorNavigation.Nombre)
        </dd>
        <dt class="col-sm-2">
            @* @Html.DisplayNameFor(model => model.IdEstatusRegistroNavigation) *@
            Estatus de Registro
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdEstatusRegistroNavigation.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-success">Editar</a>&nbsp;
    <a asp-action="Index" class="btn btn-primary">Regresar</a>
</div>
